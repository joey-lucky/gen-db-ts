{
  "name": "gen-db-ts",
  "version": "0.0.1",
  "license": "MIT",
  "author": "joey",
  "homepage": "https://github.com/joey-lucky/gen-db-ts.git",
  "bugs": "https://github.com/joey-lucky/gen-db-ts/issues",
  "description": "自动生成数据库表类型文件",
  "bin": {
    "gen-db-ts": "./bin/index.js"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --outDir lib --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:bin": "babel src --out-dir bin --extensions \".ts,.tsx\" --source-maps inline"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "commander": "^8.1.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {}
}
